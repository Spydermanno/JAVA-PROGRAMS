package PATTERNS;
import java.util.Scanner;
public class STPATTERNPRINTING {

	public STPATTERNPRINTING() {
		// TODO Auto-generated constructor stub
	}

public static void main(String[] args) {
Scanner s =new Scanner(System.in);
int n=s.nextInt();
int i=0;
while(i<=n) 
{
int j=0;
while(j<=n)
{
System.out.print('*');
j++;
}
print("\n")
i++;
}
	}

}
